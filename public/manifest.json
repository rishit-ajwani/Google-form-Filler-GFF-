{
  "manifest_version": 3,
  "name": "Google Form Filler",
  "version": "1.0",
  "description": "Auto-fill Google Forms with saved profile data.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/forms/*"],
      "js": ["content.js"]
    }
  ]
}
